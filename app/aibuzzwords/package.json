{
    "name": "aibuzzwords",
    "version": "0.0.1",
    "description": "View AI Buzzwords",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "scripts": {
        "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
        "build:cf": "ui5 build --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
        "build": "ui5 build --config ui5-deploy.yaml"
    },
    "devDependencies": {
        "@sap/ui5-builder-webide-extension": "^1.1.8",
        "@sap/ux-ui5-tooling": "^1.11.2",
        "@ui5/cli": "^3.7.0",
        "d3-cloud": "^1.2.7",
        "mbt": "^1.2.18",
        "ui5-middleware-livereload": "^3.0.2",
        "ui5-task-zipper": "^0.5.0",
        "ui5-tooling-modules": "^3.2.2"
    },
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension",
            "@sap/ux-ui5-tooling",
            "ui5-middleware-livereload",
            "ui5-task-zipper",
            "ui5-tooling-modules",
            "mbt"
        ]
    }
}

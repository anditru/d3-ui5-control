<mvc:View controllerName="aibuzzwords.controller.Main"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:custom="aibuzzwords.control">
    <Page id="page" title="{i18n>pageTitle}">
        <custom:WordCloud id="WordCloud" font="Impact" words="{/AiBuzzwords}">
            <custom:Word id="Word" text="{text}" weight="{weight}"/>
        </custom:WordCloud>
    </Page>
</mvc:View>
